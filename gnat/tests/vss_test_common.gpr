--  VSS: common packages for tests.

with "../vss_extras_common";

project VSS_Test_Common is

   for Languages use ("Ada");
   for Object_Dir use VSS_Extras_Common.Tests_Object_Dir;
   for Source_Dirs use ("../../testsuite/common");

   Ada_Compiler_Switches :=
     VSS_Extras_Common.Ada_Compiler_Switches & ("-gnatW8", "-gnat2022");

   package Compiler is
      for Switches ("Ada") use Ada_Compiler_Switches;
   end Compiler;

end VSS_Test_Common;
